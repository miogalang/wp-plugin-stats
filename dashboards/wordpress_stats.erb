<% content_for :title do %>Wordpress Stats<% end %>
<div class="gridster">
  <ul>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-view="Image" data-image="/mentor.png"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="downloads-zendesk" data-view="Downloads" data-title="Zendesk Plugin Downloads" data-value="0"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="downloads-zopim-live-chat" data-view="Downloads" data-title="Zopim Plugin Downloads" data-value="0"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
      <div data-id="downloads-inbox-by-zendesk" data-view="Downloads" data-title="Inbox Plugin Downloads" data-value="0"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1" >
      <div data-id="version-downloads-zendesk" data-view="List" data-unordered="true" data-title="Zendesk Downloads per version"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="version-downloads-zopim-live-chat" data-view="List" data-unordered="true" data-title="Zopim Downloads per version"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="daily-downloads-zendesk" data-view="Graph", data-title="Zendesk Daily Downloads" data-moreinfo="Average daily download for the past 30 days"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="daily-downloads-zopim-live-chat" data-view="Graph", data-title="Zopim Daily Downloads" data-moreinfo="Average daily download for the past 30 days"></div>
    </li>
    <li data-row="1" data-col="1" data-sizex="2" data-sizey="1">
      <div data-id="daily-downloads-inbox-by-zendesk" data-view="Graph", data-title="Inbox Daily Downloads" data-moreinfo="Average daily download for the past 30 days"></div>
    </li>
  </ul>
</div>
